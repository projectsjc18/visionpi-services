    "should": "^11.1.2",
    "snazzy": "^6.0.0",
    "standard": "^8.6.0",
    "supertest": "^2.0.1",
    "nodemon": "^1.18.10"
  },
  "dependencies": {
    "@hapi/joi": "^15.1.0",
    "awilix": "^4.2.2",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dockerode": "^2.3.1",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "http-proxy": "^1.17.0",
    "http-proxy-middleware": "^0.19.1",
    "http-status": "^0.2.5",
    "mongodb": "^3.3.0",
    "morgan": "^1.7.0",
    "node-docker-monitor": "^1.0.10",
